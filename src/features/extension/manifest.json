{
  "manifest_version": 3,
  "name": "Data Logger",
  "version": "1.0",
  "description": "Simple extension to log data to console",
  "side_panel": {
    "default_path": "popup.html"
  },
  "permissions": [
    "activeTab",
    "scripting",
    "sidePanel"
  ],
  "action": {
    "default_title": "Open Data Logger"
  },
  "background": {
    "service_worker": "background.js"
  }
}